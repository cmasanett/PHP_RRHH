            rowdata = jQuery("#" + grid).getRowData(id);
            $('#valor-posible-edit-DatosEmpVal').val(rowdata['valor_posible']);
            $('#significado-edit-DatosEmpVal').val(rowdata['significado']);
            $('#' + diagName).dialog('open');
        } else {
            $('#' + diagName).dialog('open');
        }
    }

    function clearDataDatosEmp() {
        $('#descripcion-add-DatosEmp').val("");
        $('#descripcion-edit-DatosEmp').val("");
    }

    function clearDataDatosEmpVal() {
