        }
    }

    function openDiagDatosEmp(diagName, op, grid, id) {
        clearDataDatosEmp();
        if (op == "edit") {
            rowdata = jQuery("#" + grid).getRowData(id);
            $('#descripcion-edit-DatosEmp').val(rowdata['descripcion']);
            $('#tipo-edit-DatosEmp').val(rowdata['tipo_de_campo']);
            $('#' + diagName).dialog('open');
        } else {
            $('#' + diagName).dialog('open');
            $('#tipo-add-DatosEmp option:contains("C")').prop('selected', true);
        }
    }

    function openDiagDatosEmpVal(diagName, op, grid, id) {
        clearDataDatosEmpVal();
