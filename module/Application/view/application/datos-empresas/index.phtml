            var sUrl = "<?php echo $this->url('datosemp', array('action' => 'loadDataGridVal')); ?>" + "/" + id;
            $("#gridDatosEmpVal").setGridParam({datatype:'json',url: sUrl}).trigger('reloadGrid');
        }
        else {
